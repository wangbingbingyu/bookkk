/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.wrapper {
  width: 660rpx;
  margin: 0 auto;
}

.box {
  min-height: 100%;
  height: 100%;
  background-color: #21272e;
}

.clearfix:after {
  display: block;
  height: 0;
  line-height: 0;
  contain: "";
  clear: both;
}

page {
  height: 100%;
  min-height: 100%;
  background-color: #21272e;
}
